<Window x:Class="DistributedAlgorithms.WordDocumentsPresenter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WordDocumentsPresenter" Height="800" Width="500">
    <Grid>
        <RichTextBox x:Name ="RichTextBox_document" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,0,-1">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
