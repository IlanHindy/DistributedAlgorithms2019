<Window x:Class="DistributedAlgorithms.LogFiltersSetting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DistributedAlgorithms"
        mc:Ignorable="d"
        Title="LogFiltersSetting" SizeToContent="WidthAndHeight">
    <DockPanel>
        <local:SelectControl x:Name="SelectControl" DockPanel.Dock="Top" Height="300" Message="Log Filters Setting" EnableTextBox="True"/>
        <DockPanel DockPanel.Dock="Bottom" Margin="3,0,3,0"  LastChildFill="False">
            <Button x:Name="Button_Save" Content="Save" Height="25" Click="Button_Save_Click" />
            <Button x:Name="Button_Remove" Content="Remove" Height="25" Click="Button_Remove_Click" />
            <Button x:Name="Button_Reset" Content="Reset" Height="25" Click="Button_Reset_Click" />
            <Button x:Name="Button_Quit" DockPanel.Dock="Right" Content="Quit" Height="25" Click="Button_Quit_Click" />
        </DockPanel>
    </DockPanel>
</Window>
